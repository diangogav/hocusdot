{
  "workbench.colorCustomizations": {
    // "editor.background": "#282D3F",
    // "editorGroupHeader.tabsBackground": "#282D3F",
    // "sideBar.background": "#282D3F",
    // "editorGroupHeader.tabs": "#282D3F",
    // "editorGutter.background": "#282D3F"
  },
  "editor.minimap.enabled": false,
  "workbench.editor.showTabs": "multiple",
  "editor.fontSize": 16,
  "editor.formatOnPase": false,
  "editor.suggestSelection": "first",
  "editor.rulers": [
    100
  ],
  "editor.tabSize": 2,
  "editor.wordWrap": "off",
  "editor.fontFamily": "Monaspace Radon, Bold Italic",
  "editor.fontLigatures": "'calt', 'liga', 'dlig', 'ss01', 'ss02'",
  "search.useGlobalIgnoreFiles": true,
  "search.useIgnoreFiles": true,
  "workbench.iconTheme": "sweet-vscode-icons",
  "workbench.colorTheme": "August - Material Palenight",
  "focus.opacity": "0.7",
  "focus.highlightRange": "block",
  "editor.lineHeight": 40,
  "launch": {
    "configurations": [],
    "compounds": []
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "workbench.statusBar.visible": false,
  "editor.cursorSmoothCareAnimation": "on",
  "editor.smoothScrolling": true,
  "workbench.list.smoothScrolling": true,
  "explorer.compactFolders": false,
  "editor.renderWhitespace": "all",
  "workbench.tree.indent": 20,
  "window.titleBarStyle": "custom",
  "editor.cursorBlinking": "expand",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.dragAndDrop": false,
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[typescript]": {
    "editor.formatOnSave": false,
    "editor.codeActionsOnSave": [
      "source.fixAll.eslint"
    ],
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "editor.formatOnSave": true,
  "prettier.singleQuote": true
}